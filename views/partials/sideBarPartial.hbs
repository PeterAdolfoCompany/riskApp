<div class="page-wrapper chiller-theme toggled">
  <a id="show-sidebar" class="btn btn-sm btn-dark" href="#">
    <i class="fa fa-reorder" aria-hidden="true"></i>
  </a>
  <nav id="sidebar" class="sidebar-wrapper">
    <div class="sidebar-content">
      <div class="sidebar-brand" style="background-color: #260e04">
        <a href="#"><img src="/images/logo.png" alt="Imagen" width="60px"> Risk App</a>
        <div id="close-sidebar">
          <i class="fa fa-chevron-left" aria-hidden="true"></i>
        </div>
      </div>
      <div class="sidebar-header" style="background-color: #4e342e">
        <div class="user-info">
          <span class="user-name">User
          </span>
          <span class="user-role">User Risk App Basic</span>
        </div>
      </div>
      <!-- ADD EVENT  -->
      <div class="btn-group-vertical" role="group" aria-label="Vertical button group">
        {{!-- <button type="button" class="btn btn-light ml-3" data-toggle="modal" data-target="#userSettings">
          <i class="fa fa-gear" aria-hidden="true"></i> User Settings
        </button> --}}
        <button id="PFButton" type="button" class="btn btn-light-blue ml-3" onclick=addEvent("poolFire")>
          <i class="fa fa-fire" aria-hidden="true"></i> Pool Fire
        </button>
        <button id="FBButton" type="button" class="btn btn-deep-orange ml-3" onclick=addEvent("fireBall")>
          <i class="fa fa-sun-o" aria-hidden="true"></i> Fire Ball
        </button>
        <button id="tntButton" type="button" class="btn btn-yellow ml-3" onclick=addEvent("tntExplosion")>
          <i class="fa fa-stop-circle-o" aria-hidden="true"></i> TNT Explosion
        </button>
        {{!-- <button onclick=addEvent() type="button" class="btn btn-amber ml-3">
          <i class="fa fa-map-marker" aria-hidden="true"></i>
          --- Add Event</button> --}}
        {{!-- <button onclick=actualPosition() type="button" class="btn btn-amber ml-3"><i class="fa fa-fire"
            aria-hidden="true"></i>Actual
          Position</button> --}}

      </div>

      <!-- EVENTOS  -->
      {{!-- <div class="sidebar-menu"> --}}
        <ul>

          {{#each tntExplosions}}
          <li class="media list-group-item list-group-item-secondary rgba-brown-light">
            <div class="container">
              <div class="row">
                <div class="col-sm-2">
                  <a class="rgba-brown-light white-text" href=""><i class="fa fa-edit" aria-hidden="true"></i></a>
                  <a class="rgba-brown-light white-text" href=""><i class="fa fa-trash" aria-hidden="true"></i></a>
                </div>
                <div class="col-sm-10">
                  <a class="rgba-brown-light white-text" href='#'>{{this.subsName}}</a>
                </div>
              </div>
            </div>
          </li>
          {{/each}}
        </ul>

        <!-- sidebar-FOOTER - CALC BUTTON  -->
        {{!-- <div class="sidebar-footer">
          <div class="container">

            <div class="col-12 col-md-auto">
              <button type="button" class="btn btn-success" data-toggle="modal" data-target="#fullHeightModalRight">
                CALCULATION
              </button> </div>
          </div>
        </div> --}}
      {{!-- </div> --}}
  </nav>

  {{>mapSidePr}}

  {{!-- MODAL ADD EVENT --}}
  {{>modalPoolFireEventPr}}
  {{>modalFireBallEventPr}}
  {{>modalTntExplosionEventPr}}

  {{>modalResults}}

  <!-- page-content" -->
</div>