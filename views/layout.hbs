<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{title}}</title>
    <link rel="stylesheet" href="/stylesheets/style.css"/>
    <link rel="stylesheet" href="/stylesheets/login.css"/>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Bootstrap core CSS -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.5.13/css/mdb.min.css" rel="stylesheet">
    {{!-- JS PARA EL NAVBAR --}}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <!-- Libreria para DataTables -->
    <link rel="stylesheet" type="text/css"
          href="https://cdn.datatables.net/v/bs4/dt-1.10.18/r-2.2.2/sl-1.2.6/datatables.min.css"/>
    <script type="text/javascript"
            src="https://cdn.datatables.net/v/bs4/dt-1.10.18/r-2.2.2/sl-1.2.6/datatables.min.js"></script>

</head>

<body>


{{{body}}}

<!-- ----------------------------- -->
<!-- ----SCRIPTS -APPRISK--------- -->
<!-- ----------------------------- -->
<script src="/javascripts/DBCHEM.js"></script>
<script src="/javascripts/data-tableFireBall.js"></script>
<script src="/javascripts/data-tablePoolFire.js"></script>
<script src="/javascripts/data-tableTntExplosion.js"></script>

<!-- JQuery -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<!-- Bootstrap tooltips -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.4/umd/popper.min.js"></script>
<!-- Bootstrap core JavaScript -->
<script type="text/javascript"
        src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/js/bootstrap.min.js"></script>
<!-- MDB core JavaScript -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.5.13/js/mdb.min.js"></script>
<!-- MDBootstrap Datatables  -->
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBHbjOpf5fcJI5XBdjpEe4JJT7sCeN1gXM&callback=initMap"
        async defer></script>
<script src="/javascripts/script.js"></script>
<script src="/javascripts/drawCircles.js"></script>
<script src="/javascripts/mapController.js"></script>
<script src="/javascripts/login.js"></script>
<script src="/javascripts/poolFireEvent.js"></script>
<script src="/javascripts/fireBallEvent.js"></script>
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script>
    window.onload = function() {
        let drawRadios;
        {{#each tntExplosions}}
            drawRadios = new DrawCircles(
                parseFloat({{this.location.coordinates.[1]}}),
                parseFloat({{this.location.coordinates.[0]}}),
                parseFloat({{this.radio01}}),
                parseFloat({{this.radio02}}),
                parseFloat({{this.radio03}}),
                "http://maps.google.com/mapfiles/ms/icons/yellow-dot.png"
            );

            drawRadios.draw();
        {{/each}}
        {{#each fireBall}}
            drawRadios = new DrawCircles(
                    parseFloat({{this.location.coordinates.[1]}}),
                    parseFloat({{this.location.coordinates.[0]}}),
                    parseFloat({{this.radio01}}),
                    parseFloat({{this.radio02}}),
                    parseFloat({{this.radio03}}),
                    "http://maps.google.com/mapfiles/ms/icons/red-dot.png"
            );

            drawRadios.draw();
        {{/each}}
        {{#each poolFire}}
            drawRadios = new DrawCircles(
                    parseFloat({{this.location.coordinates.[1]}}),
                    parseFloat({{this.location.coordinates.[0]}}),
                    parseFloat({{this.radio01}}),
                    parseFloat({{this.radio02}}),
                    parseFloat({{this.radio03}}),
                    "http://maps.google.com/mapfiles/ms/icons/blue-dot.png"
            );

            drawRadios.draw();
        {{/each}}
    };
</script>
</body>

</html>